{
	"name": "linkedin-cli-ts",
	"description": "The unofficial CLI and terminal client for LinkedIn (TypeScript version)",
	"version": "0.1.0",
	"license": "MIT",
	"type": "module",
	"engines": { "node": ">=20" },
	"scripts": {
		"format": "prettier --write .",
		"build": "tsc",
		"clean": "rm -rf dist",
		"lint-check": "prettier --check . && xo",
		"dev": "tsc --watch",
		"start": "node dist/cli.js"
	},
	"files": ["dist"],
	"dependencies": {
		"ink": "^6.3.1",
		"pastel": "^4.0.0",
		"react": "^19.2.0",
		"zod": "^4.1.0"
	},
	"devDependencies": {
		"typescript": "^5.0.3",
		"prettier": "^3.3.3",
		"xo": "^1.2.3",
		"@types/react": "^19.2.0"
	}
}
